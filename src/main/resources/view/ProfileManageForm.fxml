      </TableView>
      <Button fx:id="btnAdd" layoutX="480.0" layoutY="129.0" mnemonicParsing="false" onAction="#btnAddOnAction" prefHeight="39.0" prefWidth="107.0" style="-fx-background-color: #13edcc;" text="Add Profile" AnchorPane.rightAnchor="10.0" />
      <Button fx:id="btnModify" layoutX="480.0" layoutY="192.0" mnemonicParsing="false" onAction="#btnModifyOnAction" prefHeight="39.0" prefWidth="107.0" style="-fx-background-color: #13edcc;" text="Modify Profile" AnchorPane.rightAnchor="10.0" />
      <Button fx:id="btnDelete" layoutX="480.0" layoutY="252.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" prefHeight="39.0" prefWidth="107.0" style="-fx-background-color: #13edcc;" text="Delet Profile" AnchorPane.rightAnchor="10.0" />
